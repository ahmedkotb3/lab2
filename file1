mkdir lab2
cd lab2
touch file1
add file1
git init
git add file1
git commit -m "add file1"
git branch -M main
git remote add origin git@github.com:ahmedkotb3/lab2.git
git push -u origin main 
git branch dev
git branch test
git touch f1 f2
git add .
git commit -m "add files"
git push origin dev
git checkout main 
git merge dev
git push origin main

git push origin :dev
git branch -d dev

git tag -a v1 -m "version1"
git push origin v1
git tag
delete remote
git push origin --delete v1
delete local
git tag -d v1



